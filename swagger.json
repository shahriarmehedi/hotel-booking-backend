{
    "openapi": "3.0.0",
    "info": {
        "title": "Hotel booking API",
        "version": "1.0.0"
    },
    "paths": {
        "/api/v1/": {
            "get": {
                "summary": "Get a list of all the things",
                "operationId": "getThings",
                "responses": {
                    "200": {
                        "description": "A list of things",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "type": "array",
                                    "items": {
                                        "$ref": "#/components/schemas/Thing"
                                    }
                                }
                            }
                        }
                    }
                }
            }
        }
    },
    "components": {
        "schemas": {
            "Thing": {
                "type": "object",
                "properties": {
                    "id": {
                        "type": "integer",
                        "format": "int64"
                    },
                    "name": {
                        "type": "string"
                    }
                }
            }
        }
    }

}